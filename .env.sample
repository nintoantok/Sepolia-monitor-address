# Copy this file to .env and fill in your values.

# Sepolia RPC endpoints (WebSocket for live, HTTP for block/tx lookups)
WS_RPC_SEPOLIA=wss://sepolia.infura.io/ws/v3/YOUR_PROJECT_ID
HTTP_RPC_SEPOLIA=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Address to monitor (checksum or lowercase ok)
ADDRESS=0xYourSepoliaAddressHere

# Etherscan API key (Sepolia uses the sepolia subdomain but same key format)
ETHERSCAN_API_KEY=YourEtherscanKeyHere

